            <outline text="4. Reversal of
Mockery (Verses
29–36)">
                <outline text="Disbelievers
mocked believers">
                    <outline text="29">
                        <outline text="إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ
الَّذِينَ آمَنُوا يَضْحَكُونَ">
                            <outline text="Indeed, those who committed crimes used
to laugh at those who believed">
                                <outline text="إِنَّ">
                                    <outline text="Indeed"></outline>
                                </outline>
                                <outline text="الَّذِينَ">
                                    <outline text="Those who"></outline>
                                </outline>
                                <outline text="أَجْرَمُوا">
                                    <outline text="Committed crimes"></outline>
                                </outline>
                                <outline text="كَانُوا">
                                    <outline text="Used to"></outline>
                                </outline>
                                <outline text="مِنَ">
                                    <outline text="From"></outline>
                                </outline>
                                <outline text="الَّذِينَ">
                                    <outline text="Those who"></outline>
                                </outline>
                                <outline text="آمَنُوا">
                                    <outline text="Believed"></outline>
                                </outline>
                                <outline text="يَضْحَكُونَ">
                                    <outline text="Laugh"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="30">
                        <outline text="وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ">
                            <outline text="And when they passed by them, they
would wink at one another">
                                <outline text="وَإِذَا">
                                    <outline text="And when"></outline>
                                </outline>
                                <outline text="مَرُّوا">
                                    <outline text="They passed"></outline>
                                </outline>
                                <outline text="بِهِمْ">
                                    <outline text="By them"></outline>
                                </outline>
                                <outline text="يَتَغَامَزُونَ">
                                    <outline text="They would wink
at one another"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="31">
                        <outline text="وَإِذَا انْقَلَبُوا إِلَىٰ أَهْلِهِمُ انْقَلَبُوا فَكِهِينَ">
                            <outline text="And when they returned to their
people, they returned jesting">
                                <outline text="وَإِذَا">
                                    <outline text="And when"></outline>
                                </outline>
                                <outline text="انْقَلَبُوا">
                                    <outline text="They returned"></outline>
                                </outline>
                                <outline text="إِلَىٰ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="أَهْلِهِمُ">
                                    <outline text="Their people"></outline>
                                </outline>
                                <outline text="انْقَلَبُوا">
                                    <outline text="They returned"></outline>
                                </outline>
                                <outline text="فَكِهِينَ">
                                    <outline text="Jesting"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="32">
                        <outline text="وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ">
                            <outline text="And when they saw them, they said,
'Indeed, these are astray'">
                                <outline text="وَإِذَا">
                                    <outline text="And when"></outline>
                                </outline>
                                <outline text="رَأَوْهُمْ">
                                    <outline text="They saw them"></outline>
                                </outline>
                                <outline text="قَالُوا">
                                    <outline text="They said"></outline>
                                </outline>
                                <outline text="إِنَّ">
                                    <outline text="Indeed"></outline>
                                </outline>
                                <outline text="هَٰؤُلَاءِ">
                                    <outline text="These"></outline>
                                </outline>
                                <outline text="لَضَالُّونَ">
                                    <outline text="Are astray"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="33">
                        <outline text="وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ">
