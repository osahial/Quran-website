                                <outline text="فَلْيَتَنَافَسِ">
                                    <outline text="Let them compete"></outline>
                                </outline>
                                <outline text="الْمُتَنَافِسُونَ">
                                    <outline text="The competitors"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="27">
                        <outline text="وَمِزَاجُهُ مِنْ تَسْنِيمٍ">
                            <outline text="And its mixture
is of Tasnim">
                                <outline text="وَمِزَاجُهُ">
                                    <outline text="And its mixture"></outline>
                                </outline>
                                <outline text="مِنْ">
                                    <outline text="Of"></outline>
                                </outline>
                                <outline text="تَسْنِيمٍ">
                                    <outline text="Tasnim"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="28">
                        <outline text="عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ">
                            <outline text="A spring from which those
brought near drink">
                                <outline text="عَيْنًا">
                                    <outline text="A spring"></outline>
                                </outline>
                                <outline text="يَشْرَبُ">
                                    <outline text="Drink"></outline>
                                </outline>
                                <outline text="بِهَا">
                                    <outline text="From it"></outline>
                                </outline>
                                <outline text="الْمُقَرَّبُونَ">
                                    <outline text="Those brought near"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
