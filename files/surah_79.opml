<?xml version="1.0" encoding="UTF-8"?>
<opml version="1.0">
    <head>
        <title>Surah An-Nazi'at
(Surah 79)</title>
    </head>
    <body>
        <outline text="Surah An-Nazi'at
(Surah 79)">
            <outline text="1. Oath by Creation and Day of
Judgment (Verses 1–14)">
                <outline text="Swearing by
angels and forces">
                    <outline text="1">
                        <outline text="وَالنَّازِعَاتِ غَرْقًا">
                            <outline text="By those who
extract with force">
                                <outline text="وَالنَّازِعَاتِ">
                                    <outline text="And by those who extract"></outline>
                                </outline>
                                <outline text="غَرْقًا">
                                    <outline text="With force"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="2">
                        <outline text="وَالنَّاشِطَاتِ نَشْطًا">
                            <outline text="And by those who
draw out gently">
                                <outline text="وَالنَّاشِطَاتِ">
                                    <outline text="And by those
who draw out"></outline>
                                </outline>
                                <outline text="نَشْطًا">
                                    <outline text="Gently"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="3">
                        <outline text="وَالسَّابِحَاتِ سَبْحًا">
                            <outline text="And by those who
glide swiftly">
                                <outline text="وَالسَّابِحَاتِ">
                                    <outline text="And by those who glide"></outline>
                                </outline>
                                <outline text="سَبْحًا">
                                    <outline text="Swiftly"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="4">
                        <outline text="فَالسَّابِقَاتِ سَبْقًا">
                            <outline text="Then those who
race ahead swiftly">
                                <outline text="فَالسَّابِقَاتِ">
                                    <outline text="Then those who
race ahead"></outline>
                                </outline>
                                <outline text="سَبْقًا">
                                    <outline text="Swiftly"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="5">
                        <outline text="فَالْمُدَبِّرَاتِ أَمْرًا">
                            <outline text="Then those who
arrange the matter">
                                <outline text="فَالْمُدَبِّرَاتِ">
                                    <outline text="Then those who arrange"></outline>
                                </outline>
                                <outline text="أَمْرًا">
                                    <outline text="The matter"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="The Day of Resurrection">
                    <outline text="6">
                        <outline text="يَوْمَ تَرْجُفُ الرَّاجِفَةُ">
                            <outline text="The Day when the
quaking quakes">
                                <outline text="يَوْمَ">
                                    <outline text="The Day"></outline>
                                </outline>
                                <outline text="تَرْجُفُ">
                                    <outline text="Quakes"></outline>
                                </outline>
                                <outline text="الرَّاجِفَةُ">
                                    <outline text="The quaking"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="7">
                        <outline text="تَتْبَعُهَا الرَّادِفَةُ">
                            <outline text="Followed by the
subsequent">
                                <outline text="تَتْبَعُهَا">
                                    <outline text="Followed by it"></outline>
                                </outline>
                                <outline text="الرَّادِفَةُ">
                                    <outline text="The subsequent"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="8">
                        <outline text="قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ">
                            <outline text="Hearts that Day will
be trembling">
                                <outline text="قُلُوبٌ">
                                    <outline text="Hearts"></outline>
                                </outline>
                                <outline text="يَوْمَئِذٍ">
                                    <outline text="That Day"></outline>
                                </outline>
                                <outline text="وَاجِفَةٌ">
                                    <outline text="Trembling"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="9">
                        <outline text="أَبْصَارُهَا خَاشِعَةٌ">
                            <outline text="Their eyes humbled">
                                <outline text="أَبْصَارُهَا">
                                    <outline text="Their eyes"></outline>
                                </outline>
                                <outline text="خَاشِعَةٌ">
                                    <outline text="Humbled"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Denial and reality">
                    <outline text="10">
                        <outline text="يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ">
                            <outline text="They say, 'Will we indeed be
returned to the former state'">
                                <outline text="يَقُولُونَ">
                                    <outline text="They say"></outline>
                                </outline>
                                <outline text="أَإِنَّا">
                                    <outline text="Will we indeed"></outline>
                                </outline>
                                <outline text="لَمَرْدُودُونَ">
                                    <outline text="Be returned"></outline>
                                </outline>
                                <outline text="فِي">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="الْحَافِرَةِ">
                                    <outline text="The former state"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="11">
                        <outline text="أَإِذَا كُنَّا عِظَامًا نَخِرَةً">
                            <outline text="When we are
decayed bones">
                                <outline text="أَإِذَا">
                                    <outline text="When"></outline>
                                </outline>
                                <outline text="كُنَّا">
                                    <outline text="We are"></outline>
                                </outline>
                                <outline text="عِظَامًا">
                                    <outline text="Bones"></outline>
                                </outline>
                                <outline text="نَخِرَةً">
                                    <outline text="Decayed"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="12">
                        <outline text="قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ">
                            <outline text="They say, 'That, then,
would be a losing return'">
                                <outline text="قَالُوا">
                                    <outline text="They say"></outline>
                                </outline>
                                <outline text="تِلْكَ">
                                    <outline text="That"></outline>
                                </outline>
                                <outline text="إِذًا">
                                    <outline text="Then"></outline>
                                </outline>
                                <outline text="كَرَّةٌ">
                                    <outline text="A return"></outline>
                                </outline>
                                <outline text="خَاسِرَةٌ">
                                    <outline text="Losing"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="13">
                        <outline text="فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ">
                            <outline text="But it will be only a
single shout">
                                <outline text="فَإِنَّمَا">
                                    <outline text="But indeed"></outline>
                                </outline>
                                <outline text="هِيَ">
                                    <outline text="It is"></outline>
                                </outline>
                                <outline text="زَجْرَةٌ">
                                    <outline text="A shout"></outline>
                                </outline>
                                <outline text="وَاحِدَةٌ">
                                    <outline text="Single"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="14">
                        <outline text="فَإِذَا هُمْ بِالسَّاهِرَةِ">
                            <outline text="And suddenly they will be
upon the surface">
                                <outline text="فَإِذَا">
                                    <outline text="And suddenly"></outline>
                                </outline>
                                <outline text="هُمْ">
                                    <outline text="They"></outline>
                                </outline>
                                <outline text="بِالسَّاهِرَةِ">
                                    <outline text="Upon the surface"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
            </outline>
            <outline text="2. Story of Musa and
Firawn (Verses 15–26)">
                <outline text="Musa’s mission">
                    <outline text="15">
                        <outline text="هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ">
                            <outline text="Has there reached you
the story of Musa">
                                <outline text="هَلْ">
                                    <outline text="Has"></outline>
                                </outline>
                                <outline text="أَتَاكَ">
                                    <outline text="Reached you"></outline>
                                </outline>
                                <outline text="حَدِيثُ">
                                    <outline text="The story"></outline>
                                </outline>
                                <outline text="مُوسَىٰ">
                                    <outline text="Of Musa"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="16">
                        <outline text="إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى">
                            <outline text="When his Lord called to him in
the sacred valley of Tuwa">
                                <outline text="إِذْ">
                                    <outline text="When"></outline>
                                </outline>
                                <outline text="نَادَاهُ">
                                    <outline text="Called to him"></outline>
                                </outline>
                                <outline text="رَبُّهُ">
                                    <outline text="His Lord"></outline>
                                </outline>
                                <outline text="بِالْوَادِ">
                                    <outline text="In the valley"></outline>
                                </outline>
                                <outline text="الْمُقَدَّسِ">
                                    <outline text="Sacred"></outline>
                                </outline>
                                <outline text="طُوًى">
                                    <outline text="Of Tuwa"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Command to Firawn">
                    <outline text="17">
                        <outline text="اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ">
                            <outline text="Go to Firawn, indeed he
has transgressed">
                                <outline text="اذْهَبْ">
                                    <outline text="Go"></outline>
                                </outline>
                                <outline text="إِلَىٰ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="فِرْعَوْنَ">
                                    <outline text="Firawn"></outline>
                                </outline>
                                <outline text="إِنَّهُ">
                                    <outline text="Indeed he"></outline>
                                </outline>
                                <outline text="طَغَىٰ">
                                    <outline text="Has transgressed"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="18">
                        <outline text="فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ">
                            <outline text="And say, 'Would you
purify yourself'">
                                <outline text="فَقُلْ">
                                    <outline text="And say"></outline>
                                </outline>
                                <outline text="هَلْ">
                                    <outline text="Would"></outline>
                                </outline>
                                <outline text="لَكَ">
                                    <outline text="You"></outline>
                                </outline>
                                <outline text="إِلَىٰ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="أَنْ">
                                    <outline text="That"></outline>
                                </outline>
                                <outline text="تَزَكَّىٰ">
                                    <outline text="Purify yourself"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="19">
                        <outline text="وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ">
                            <outline text="And I will guide you to your
Lord so you may fear">
                                <outline text="وَأَهْدِيَكَ">
                                    <outline text="And I will guide you"></outline>
                                </outline>
                                <outline text="إِلَىٰ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="رَبِّكَ">
                                    <outline text="Your Lord"></outline>
                                </outline>
                                <outline text="فَتَخْشَىٰ">
                                    <outline text="So you may fear"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Firawn’s rejection">
                    <outline text="20">
                        <outline text="فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ">
                            <outline text="Then he showed him
the greatest sign">
                                <outline text="فَأَرَاهُ">
                                    <outline text="Then he showed him"></outline>
                                </outline>
                                <outline text="الْآيَةَ">
                                    <outline text="The sign"></outline>
                                </outline>
                                <outline text="الْكُبْرَىٰ">
                                    <outline text="Greatest"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="21">
                        <outline text="فَكَذَّبَ وَعَصَىٰ">
                            <outline text="But he denied
and disobeyed">
                                <outline text="فَكَذَّبَ">
                                    <outline text="But he denied"></outline>
                                </outline>
                                <outline text="وَعَصَىٰ">
                                    <outline text="And disobeyed"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="22">
                        <outline text="ثُمَّ أَدْبَرَ يَسْعَىٰ">
                            <outline text="Then he turned
away, striving">
                                <outline text="ثُمَّ">
                                    <outline text="Then"></outline>
                                </outline>
                                <outline text="أَدْبَرَ">
                                    <outline text="He turned away"></outline>
                                </outline>
                                <outline text="يَسْعَىٰ">
                                    <outline text="Striving"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="23">
                        <outline text="فَحَشَرَ فَنَادَىٰ">
                            <outline text="And gathered
and called out">
                                <outline text="فَحَشَرَ">
                                    <outline text="And gathered"></outline>
                                </outline>
                                <outline text="فَنَادَىٰ">
                                    <outline text="And called out"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="24">
                        <outline text="فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ">
                            <outline text="And said, 'I am your
Lord, the Most High'">
                                <outline text="فَقَالَ">
                                    <outline text="And said"></outline>
                                </outline>
                                <outline text="أَنَا">
                                    <outline text="I am"></outline>
                                </outline>
                                <outline text="رَبُّكُمُ">
                                    <outline text="Your Lord"></outline>
                                </outline>
                                <outline text="الْأَعْلَىٰ">
                                    <outline text="The Most High"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Firawn’s punishment">
                    <outline text="25">
                        <outline text="فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ">
                            <outline text="So Allah seized him with punishment of
the Hereafter and the first">
                                <outline text="فَأَخَذَهُ">
                                    <outline text="So seized him"></outline>
                                </outline>
                                <outline text="اللَّهُ">
                                    <outline text="Allah"></outline>
                                </outline>
                                <outline text="نَكَالَ">
                                    <outline text="With punishment"></outline>
                                </outline>
                                <outline text="الْآخِرَةِ">
                                    <outline text="Of the Hereafter"></outline>
                                </outline>
                                <outline text="وَالْأُولَىٰ">
                                    <outline text="And the first"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="26">
                        <outline text="إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ">
                            <outline text="Indeed, in that is a lesson
for whoever fears">
                                <outline text="إِنَّ">
                                    <outline text="Indeed"></outline>
                                </outline>
                                <outline text="فِي">
                                    <outline text="In"></outline>
                                </outline>
                                <outline text="ذَٰلِكَ">
                                    <outline text="That"></outline>
                                </outline>
                                <outline text="لَعِبْرَةً">
                                    <outline text="Is a lesson"></outline>
                                </outline>
                                <outline text="لِمَنْ">
                                    <outline text="For whoever"></outline>
                                </outline>
                                <outline text="يَخْشَىٰ">
                                    <outline text="Fears"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
            </outline>
            <outline text="3. Creation, Judgment, and
Consequences (Verses 27–46)">
                <outline text="Challenge of creation">
                    <outline text="27">
                        <outline text="أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ بَنَاهَا">
                            <outline text="Are you a more difficult creation or
the heaven He constructed">
                                <outline text="أَأَنْتُمْ">
                                    <outline text="Are you"></outline>
                                </outline>
                                <outline text="أَشَدُّ">
                                    <outline text="More difficult"></outline>
                                </outline>
                                <outline text="خَلْقًا">
                                    <outline text="Creation"></outline>
                                </outline>
                                <outline text="أَمِ">
                                    <outline text="Or"></outline>
                                </outline>
                                <outline text="السَّمَاءُ">
                                    <outline text="The heaven"></outline>
                                </outline>
                                <outline text="بَنَاهَا">
                                    <outline text="He constructed"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="28">
                        <outline text="رَفَعَ سَمْكَهَا فَسَوَّاهَا">
                            <outline text="He raised its ceiling
and proportioned it">
                                <outline text="رَفَعَ">
                                    <outline text="He raised"></outline>
                                </outline>
                                <outline text="سَمْكَهَا">
                                    <outline text="Its ceiling"></outline>
                                </outline>
                                <outline text="فَسَوَّاهَا">
                                    <outline text="And proportioned it"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="29">
                        <outline text="وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا">
                            <outline text="And He darkened its night and
brought forth its morning">
                                <outline text="وَأَغْطَشَ">
                                    <outline text="And He darkened"></outline>
                                </outline>
                                <outline text="لَيْلَهَا">
                                    <outline text="Its night"></outline>
                                </outline>
                                <outline text="وَأَخْرَجَ">
                                    <outline text="And brought forth"></outline>
                                </outline>
                                <outline text="ضُحَاهَا">
                                    <outline text="Its morning"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="30">
                        <outline text="وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا">
                            <outline text="And the earth, after
that, He spread it">
                                <outline text="وَالْأَرْضَ">
                                    <outline text="And the earth"></outline>
                                </outline>
                                <outline text="بَعْدَ">
                                    <outline text="After"></outline>
                                </outline>
                                <outline text="ذَٰلِكَ">
                                    <outline text="That"></outline>
                                </outline>
                                <outline text="دَحَاهَا">
                                    <outline text="He spread it"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="31">
                        <outline text="أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا">
                            <outline text="He extracted from it its
water and its pasture">
                                <outline text="أَخْرَجَ">
                                    <outline text="He extracted"></outline>
                                </outline>
                                <outline text="مِنْهَا">
                                    <outline text="From it"></outline>
                                </outline>
                                <outline text="مَاءَهَا">
                                    <outline text="Its water"></outline>
                                </outline>
                                <outline text="وَمَرْعَاهَا">
                                    <outline text="And its pasture"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="32">
                        <outline text="وَالْجِبَالَ أَرْسَاهَا">
                            <outline text="And the mountains
He anchored">
                                <outline text="وَالْجِبَالَ">
                                    <outline text="And the mountains"></outline>
                                </outline>
                                <outline text="أَرْسَاهَا">
                                    <outline text="He anchored"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Purpose of creation">
                    <outline text="33">
                        <outline text="مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ">
                            <outline text="As provision for you
and your livestock">
                                <outline text="مَتَاعًا">
                                    <outline text="As provision"></outline>
                                </outline>
                                <outline text="لَكُمْ">
                                    <outline text="For you"></outline>
                                </outline>
                                <outline text="وَلِأَنْعَامِكُمْ">
                                    <outline text="And for your livestock"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="The Day of Decision">
                    <outline text="34">
                        <outline text="فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ">
                            <outline text="But when the
overwhelming event
comes">
                                <outline text="فَإِذَا">
                                    <outline text="But when"></outline>
                                </outline>
                                <outline text="جَاءَتِ">
                                    <outline text="Comes"></outline>
                                </outline>
                                <outline text="الطَّامَّةُ">
                                    <outline text="The overwhelming event"></outline>
                                </outline>
                                <outline text="الْكُبْرَىٰ">
                                    <outline text="Great"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="35">
                        <outline text="يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ">
                            <outline text="The Day when man will
remember what he strove for">
                                <outline text="يَوْمَ">
                                    <outline text="The Day"></outline>
                                </outline>
                                <outline text="يَتَذَكَّرُ">
                                    <outline text="Will remember"></outline>
                                </outline>
                                <outline text="الْإِنْسَانُ">
                                    <outline text="Man"></outline>
                                </outline>
                                <outline text="مَا">
                                    <outline text="What"></outline>
                                </outline>
                                <outline text="سَعَىٰ">
                                    <outline text="He strove for"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="36">
                        <outline text="وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ">
                            <outline text="And Hellfire will be
exposed for whoever sees">
                                <outline text="وَبُرِّزَتِ">
                                    <outline text="And will be exposed"></outline>
                                </outline>
                                <outline text="الْجَحِيمُ">
                                    <outline text="Hellfire"></outline>
                                </outline>
                                <outline text="لِمَنْ">
                                    <outline text="For whoever"></outline>
                                </outline>
                                <outline text="يَرَىٰ">
                                    <outline text="Sees"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Fates of the
rebellious and
righteous">
                    <outline text="37">
                        <outline text="فَأَمَّا مَنْ طَغَىٰ">
                            <outline text="As for he who
transgressed">
                                <outline text="فَأَمَّا">
                                    <outline text="As for"></outline>
                                </outline>
                                <outline text="مَنْ">
                                    <outline text="He who"></outline>
                                </outline>
                                <outline text="طَغَىٰ">
                                    <outline text="Transgressed"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="38">
                        <outline text="وَآثَرَ الْحَيَاةَ الدُّنْيَا">
                            <outline text="And preferred the
life of this world">
                                <outline text="وَآثَرَ">
                                    <outline text="And preferred"></outline>
                                </outline>
                                <outline text="الْحَيَاةَ">
                                    <outline text="The life"></outline>
                                </outline>
                                <outline text="الدُّنْيَا">
                                    <outline text="Of this world"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="39">
                        <outline text="فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ">
                            <outline text="Then indeed,
Hellfire is the abode">
                                <outline text="فَإِنَّ">
                                    <outline text="Then indeed"></outline>
                                </outline>
                                <outline text="الْجَحِيمَ">
                                    <outline text="Hellfire"></outline>
                                </outline>
                                <outline text="هِيَ">
                                    <outline text="Is"></outline>
                                </outline>
                                <outline text="الْمَأْوَىٰ">
                                    <outline text="The abode"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="40">
                        <outline text="وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ
وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ">
                            <outline text="But as for he who feared the
standing before his Lord and
restrained the soul from desire">
                                <outline text="وَأَمَّا">
                                    <outline text="But as for"></outline>
                                </outline>
                                <outline text="مَنْ">
                                    <outline text="He who"></outline>
                                </outline>
                                <outline text="خَافَ">
                                    <outline text="Feared"></outline>
                                </outline>
                                <outline text="مَقَامَ">
                                    <outline text="The standing"></outline>
                                </outline>
                                <outline text="رَبِّهِ">
                                    <outline text="Before his Lord"></outline>
                                </outline>
                                <outline text="وَنَهَى">
                                    <outline text="And restrained"></outline>
                                </outline>
                                <outline text="النَّفْسَ">
                                    <outline text="The soul"></outline>
                                </outline>
                                <outline text="عَنِ">
                                    <outline text="From"></outline>
                                </outline>
                                <outline text="الْهَوَىٰ">
                                    <outline text="Desire"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="41">
                        <outline text="فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ">
                            <outline text="Then indeed,
Paradise is the
abode">
                                <outline text="فَإِنَّ">
                                    <outline text="Then indeed"></outline>
                                </outline>
                                <outline text="الْجَنَّةَ">
                                    <outline text="Paradise"></outline>
                                </outline>
                                <outline text="هِيَ">
                                    <outline text="Is"></outline>
                                </outline>
                                <outline text="الْمَأْوَىٰ">
                                    <outline text="The abode"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
                <outline text="Uncertainty of the Hour">
                    <outline text="42">
                        <outline text="يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا">
                            <outline text="They ask you about the
Hour, when is its arrival">
                                <outline text="يَسْأَلُونَكَ">
                                    <outline text="They ask you"></outline>
                                </outline>
                                <outline text="عَنِ">
                                    <outline text="About"></outline>
                                </outline>
                                <outline text="السَّاعَةِ">
                                    <outline text="The Hour"></outline>
                                </outline>
                                <outline text="أَيَّانَ">
                                    <outline text="When"></outline>
                                </outline>
                                <outline text="مُرْسَاهَا">
                                    <outline text="Is its arrival"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="43">
                        <outline text="فِيمَ أَنْتَ مِنْ ذِكْرَاهَا">
                            <outline text="In what position are
you to mention it">
                                <outline text="فِيمَ">
                                    <outline text="In what"></outline>
                                </outline>
                                <outline text="أَنْتَ">
                                    <outline text="Are you"></outline>
                                </outline>
                                <outline text="مِنْ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="ذِكْرَاهَا">
                                    <outline text="Mention it"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="44">
                        <outline text="إِلَىٰ رَبِّكَ مُنْتَهَاهَا">
                            <outline text="To your Lord is its finality">
                                <outline text="إِلَىٰ">
                                    <outline text="To"></outline>
                                </outline>
                                <outline text="رَبِّكَ">
                                    <outline text="Your Lord"></outline>
                                </outline>
                                <outline text="مُنْتَهَاهَا">
                                    <outline text="Its finality"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="45">
                        <outline text="إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا">
                            <outline text="You are only a warner
for whoever fears it">
                                <outline text="إِنَّمَا">
                                    <outline text="Indeed"></outline>
                                </outline>
                                <outline text="أَنْتَ">
                                    <outline text="You are"></outline>
                                </outline>
                                <outline text="مُنْذِرُ">
                                    <outline text="A warner"></outline>
                                </outline>
                                <outline text="مَنْ">
                                    <outline text="For whoever"></outline>
                                </outline>
                                <outline text="يَخْشَاهَا">
                                    <outline text="Fears it"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                    <outline text="46">
                        <outline text="كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا
إِلَّا عَشِيَّةً أَوْ ضُحَاهَا">
                            <outline text="It will be as if, on the Day they see it,
they had not remained except an
evening or its morning">
                                <outline text="كَأَنَّهُمْ">
                                    <outline text="As if they"></outline>
                                </outline>
                                <outline text="يَوْمَ">
                                    <outline text="On the Day"></outline>
                                </outline>
                                <outline text="يَرَوْنَهَا">
                                    <outline text="They see it"></outline>
                                </outline>
                                <outline text="لَمْ">
                                    <outline text="Not"></outline>
                                </outline>
                                <outline text="يَلْبَثُوا">
                                    <outline text="They had remained"></outline>
                                </outline>
                                <outline text="إِلَّا">
                                    <outline text="Except"></outline>
                                </outline>
                                <outline text="عَشِيَّةً">
                                    <outline text="An evening"></outline>
                                </outline>
                                <outline text="أَوْ">
                                    <outline text="Or"></outline>
                                </outline>
                                <outline text="ضُحَاهَا">
                                    <outline text="Its morning"></outline>
                                </outline>
                            </outline>
                        </outline>
                    </outline>
                </outline>
            </outline>
        </outline>
